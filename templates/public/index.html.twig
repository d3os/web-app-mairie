{% extends 'base.html.twig' %}

{% block title %}Hello PublicController!{% endblock %}

{% block body %}


    
     {% for new in news_list %}

    
        <twig:Inset type="{{ new.type|e }}" title="{{ new.title|e }}" link="{{ new.link|e }}" imgurl="{{ new.imgurl|e }}" imgalt="{{ new.imgalt|e }}" />
        {% else %} {# if no entities found #}
        <em>no data found</em>

    {% endfor %}

{% endblock %}
